        },
        "question_FYFecm": {
          "type": "question",
          "settings": {
            "heading": "What types of credit cards do you accept?",
            "icon": "none",
            "row_content": "<p>We accept Visa, MasterCard and American Express cards. Alternatively, you can finalize your order with PayPal payment service.<\/p>",
            "page": ""
          }
        }
