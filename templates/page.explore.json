    "content_tabs_Pik6nD": {
      "type": "content-tabs",
      "blocks": {
        "tab_gmwAaR": {
          "type": "tab",
          "settings": {
            "image_size": "landscape-v2",
            "image_placement": "left",
            "nav_label": "2000",
            "subheading": "",
            "heading": "The Birth of the Brand",
            "heading_size": "h2",
            "content": "<p>The fashion brand was founded with a mission to bring a fresh perspective to the industry, blending creativity with practicality. From the very beginning, we set out to define trends and craft designs that carry a unique identity.<\/p>",
            "button_label": "",
            "button_link": "",
